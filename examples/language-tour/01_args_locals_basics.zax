func add_words(left_word: word, right_word: word): word
  ld hl, left_word
  ld de, right_word
  add hl, de
end

func bump_byte(input_byte: byte): byte
  var
    temp_word: word = 0
  end

  ld l, input_byte
  ld h, 0
  inc l
  ld temp_word, hl
  ld hl, temp_word
end

export func main(): void
  var
    result_word: word = 0
  end

  add_words 10, 20
  ld result_word, hl

  bump_byte 7
end
