section code at $0000
section var at $1000

extern func getb(seed: byte): byte at $1234
extern func getw(seed: word): word at $1240
extern func ping(seed: byte): void at $1250

globals
  idx: byte
  arr: byte[16]
  out: word

export func main(): void
  ping 1
  getb arr[idx]
  ld a, l
  getw 9
  ld out, hl
  ret
end

