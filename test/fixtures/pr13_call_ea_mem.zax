; PR13 fixture: pass ea and (ea) as call args
section code at $0000
section data at $0040

extern func takeWord(w: word): void at $1234
extern func takeByte(b: byte): void at $1234

data
  w: word = [ $BEEF ]
  b: byte = [ $7F ]

export func main(): void
  asm
    takeWord w
    takeWord (w)
    takeByte (b)
    ret
end

