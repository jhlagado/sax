section var at $8000

globals
  w: word
  b: byte
  w2: word

export func main(): void
  asm
    ld b, c
    ld c, d
    ld d, e
    ld e, h
    ld h, l
    ld l, a
    ld a, b
    ld b, (hl)
    ld (hl), c
    ld (hl), $7F
    ld a, (bc)
    ld (de), a
    ld hl, $2345
    ld a, (w)
    ld (w2), a
    ld (w), hl
    ld sp, $3456
    ld sp, hl
    push af
    pop af
    ex de, hl
    ex (sp), hl
    di
    ei
    cpl
    scf
    ccf
    nop
    halt
  end
