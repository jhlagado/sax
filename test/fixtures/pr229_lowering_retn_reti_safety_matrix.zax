func local_retn(): void
  var
    scratch: byte
  end
  retn
end

func local_reti(): void
  var
    scratch: byte
  end
  reti
end

func arg_nonzero_retn(a: byte): void
  push bc
  retn
end

func arg_untracked_reti(a: byte): void
  ld sp, $1000
  reti
end

func arg_unknown_retn(a: byte): void
  select a
    case 0
      push bc
    case 1
      nop
  end
  retn
end
