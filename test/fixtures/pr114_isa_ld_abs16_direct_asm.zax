section code at $0000
section var at $1000

globals
  w0: word
  w1: word
  w2: word
  w3: word
  w4: word
  w5: word
  w6: word
  w7: word
  b0: byte

export func main(): void
  asm
    ld a, (b0)
    ld (b0), a
    ld hl, (w0)
    ld (w0), hl
    ld bc, (w1)
    ld (w1), bc
    ld de, (w2)
    ld (w2), de
    ld sp, (w3)
    ld (w3), sp
    ld ix, (w4)
    ld (w4), ix
    ld iy, (w5)
    ld (w5), iy
end
